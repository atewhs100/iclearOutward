package com.strivers.A2Z.countdigits;

public class ArmstrongNumber {

	public static void main(String[] args) {

		int number = 132;
		ArmstrongNumber.findArmstrongNumber(number);
	}

	private static boolean findArmstrongNumber(int number) {

		int calculateArmstrong = 0;

		while (number > 0) {

			number = number % 10;
			calculateArmstrong = calculateArmstrong + number * number * number;
			number = number / 10;

		}

		if (calculateArmstrong == )
		return false;

	}
}
